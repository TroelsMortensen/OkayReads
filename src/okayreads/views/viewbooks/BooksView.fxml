<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane fx:id="mainPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="okayreads.views.viewbooks.BooksViewController">
    <top>
        <HBox prefHeight="25.0" prefWidth="998.0" BorderPane.alignment="CENTER">
            <children>
                <Button mnemonicParsing="false" text="Filter" />
            </children>
        </HBox>
    </top>
    <center>
        <TableView BorderPane.alignment="CENTER">
            <columns>
                <TableColumn prefWidth="75.0" text="C1" />
                <TableColumn prefWidth="75.0" text="C2" />
            </columns>
        </TableView>
    </center>
    <bottom>
        <HBox prefHeight="25.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <children>
                <Button mnemonicParsing="false" text="Previous" />
                <Button mnemonicParsing="false" text="Next" />
            </children>
        </HBox>
    </bottom>
</BorderPane>
